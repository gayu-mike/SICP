#lang sicp

(list 1 (list 2 (list 3 4)))

;; 打印的结果
(1 (2 (3 4)))

;; box-pointer
;; * for pointer
;; / for nil
[*][*] -> [*][/]                (1 (2 (3 4)))
 |         |
 v         v
 1        [*][*] -> [*][/]         (2 (3 4))
           |         |
           v         v
           2        [*][*] -> [*][/]  (3 4)
                     |         |
                     v         v
                     3         4

;; 树状图
  ^  (1 (2 (3 4)))
 / \
1   ^   (2 (3 4))
   / \
  2   ^    (3 4)
     / \
    3   4